<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="ltr">
<!-- BEGIN: Head-->

<head>

    <title>Dashoard</title>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600" rel="stylesheet">

    <!--Theme CSS-->
    <link rel="stylesheet" type="text/css" href="/stylesheets/app-assets/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/app-assets/css/bootstrap-extended.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/app-assets/css/components.css">

    <!-- Page CSS-->
    <link rel="stylesheet" type="text/css" href="app-assets/css/core/menu/menu-types/vertical-menu.css">
    <link rel="stylesheet" type="text/css" href="app-assets/css/pages/dashboard-analytics.css">
    <link rel="stylesheet" type="text/css" href="app-assets/css/pages/card-analytics.css">

    <!-- Custom CSS-->
    <link rel="stylesheet" type="text/css" href="/stylesheets/newstyle.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/sidebar-style.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/navbar-style.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/main-page.css">

</head>
<!-- END: Head-->

<body class="vertical-layout vertical-menu-modern 2-columns  navbar-floating footer-static" data-open="click" data-menu="vertical-menu-modern" data-col="2-columns">

    <nav class="header-navbar navbar-expand-lg navbar navbar-with-menu floating-nav navbar-light navbar-shadow">
        <div class="navbar-wrapper">
            <div class="navbar-container content">
                <div class="navbar-collapse" id="navbar-mobile">
                    <div class="mr-auto float-left bookmark-wrapper d-flex align-items-center">
                        <ul class="nav navbar-nav">
                            <li class="nav-item mobile-menu d-xl-none mr-auto"><a class="nav-link nav-menu-main menu-toggle hidden-xs" href="#"><i
                                        class="ficon feather icon-menu"></i></a></li>
                        </ul>
                        <ul class="nav navbar-nav search-icon">
                            <li class="nav-item nav-search"><a class="nav-link nav-link-search"><i
                                        class="ficon feather icon-search"></i></a>
                                <div class="search-input">
                                    <div class="search-input-icon"><i class="feather icon-search primary"></i></div>

                                    <div class="search-input-close"><i class="feather icon-x"></i></div>
                                    <ul class="search-list search-list-main"></ul>
                                </div>
                            </li>
                        </ul>
                        <ul class="nav navbar-nav">
                            <!--   option Calendar-->
                            </li>
                        </ul>
                    </div>
                    <ul class="nav navbar-nav float-right notifcation">
                        <li class="nav-item">
                            <a class=" nav-link dropdown-user-link" href="#">
                                <span><img class="round"  src="images/avatar.png"
                                        alt="avatar" height="40" width="40"></span>
                            </a>

                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>

    <ul class="main-search-list-defaultlist d-none">
    </ul>
    <ul class="main-search-list-defaultlist-other-list d-none">
        <li class="auto-suggestion d-flex align-items-center justify-content-between cursor-pointer">
            <a class="d-flex align-items-center justify-content-between w-100 py-50">
                <div class="d-flex justify-content-start"><span class="mr-75 feather icon-alert-circle"></span><span>No results found.</span></div>
            </a>
        </li>
    </ul>


    <!-- Main Menu -->
    <div class="main-menu menu-fixed menu-light menu-accordion menu-shadow" data-scroll-to-active="true">
        <div class="navbar-header">
            <ul class="nav navbar-nav flex-row">
                <li class="nav-item mr-auto">
                    <a class="navbar-brand" href="#">
                        <h2 class="brand-text mb-0 text-white">Hospital</h2>
                    </a>
                </li>
                <li class="nav-item nav-toggle"><a class="nav-link modern-nav-toggle pr-0" data-toggle="collapse"><i class="feather icon-x d-block d-xl-none font-medium-4 primary toggle-icon"></i><i class="toggle-icon feather icon-disc font-medium-4 d-none  d-xl-block collapse-toggle-icon primary" data-ticon="icon-disc" style="color: aliceblue !important;"></i></a></li>
            </ul>
        </div>
        <div class="main-menu-content">
            <ul class="navigation navigation-main" id="main-menu-navigation" data-menu="menu-navigation">
                <li class="nav-item  mt-1 active"><a href="main"><i class="feather icon-home"></i><span class="menu-item" data-i18n="Analytics">Home</span></a>
                </li>
                <li class="nav-item mt-1 "><a href="doctors"><i class="feather icon-user"></i><span class="menu-title" data-i18n="Users">Doctors</span></a>
                </li>
                <li class="nav-item mt-1"><a href="nurse"><i class="feather icon-pocket"></i><span class="menu-title" data-i18n="Roles">Nurse</span></a>
                </li>
                <li class=" nav-item mt-3"><a href="login-page"><i class="feather icon-log-out"></i><span class="menu-title" data-i18n="Settings">Logout</span></a>
                </li>
            </ul>
        </div>
    </div>
    <!-- Main Menu -->

    <!-- Content-->
    <div class="app-content content">
        <div class="content-overlay"></div>
        <div class="header-navbar-shadow"></div>
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body">
                <!-- Dashboard Analytics Start -->
                <section id="dashboard-analytics">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="statics-cuolums">
                                <div class="row d-flex align-items-start justify-content-start">

                                    <% HospitalList.forEach((attribute)=>{%>

                                        <div class="col-lg-4 col-sm-12">
                                            <div class="card card-active-user">
                                                <div class="card-header d-flex align-items-start pb-0">
                                                    <div>
                                                        <h5 class="tmb-0">
                                                            <%=attribute.name %>
                                                        </h5>
                                                        <p class="text-bold-700">
                                                            <%=attribute.size %>
                                                        </p>
                                                    </div>
                                                    <div class="avatar bg-rgba-success p-50 m-0">
                                                        <div class="avatar-content">
                                                            <a href='#'> <img src="icons/Group 437.svg" alt=""> </a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                        <% }) %>

                                </div>

                            </div>
                        </div>
                    </div>
                </section>
                <!-- Dashboard Analytics end -->

            </div>
        </div>
    </div>

</body>

</html>